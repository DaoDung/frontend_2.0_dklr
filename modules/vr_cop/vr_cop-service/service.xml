	<namespace>vrcop</namespace>
	
	<entity local-service="true" name="VRCOPReportRepository" remote-service="false" uuid="false" table="vr_copreportrepository">

		<column name="id" primary="true" type="long" db-name="id" />
		
		<column name="mtCore" type="int" />
		<column name="applicantProfileId" type="long" />
		<column name="applicantCode" type="String" />
		<column name="applicantName" type="String" />
		<column name="applicantAddress" type="String" />
		<column name="overseasManufacturerCode" type="String" />
		<column name="overseasManufacturerName" type="String" />
		<column name="overseasManufacturerAddress" type="String" />
		
		<column name="productionPlantId" type="long" />
		<column name="productionPlantCode" type="String" />
		<column name="productionPlantName" type="String" />
		<column name="productionPlantAddress" type="String" />
		
		<column name="COPReportNo" type="String" />
		<column name="COPReportStatus" type="String" />
		<column name="COPReportType" type="String" />
		<column name="COPReportMetadata" type="String" />
		<column name="COPReportSignName" type="String" />
		<column name="COPReportSignTitle" type="String" />
		<column name="COPReportSignPlace" type="String" />
		<column name="COPReportDate" type="Date" />
		<column name="COPReportApprovedDate" type="Date" />
		<column name="COPReportExpiredDate" type="Date" />
		<column name="COPFileEntryId" type="long" />
		
		<column name="modifyDate" type="Date" />
		<column name="syncDate" type="Date" />
		

		<order by="desc">
			<order-column name="id" />
		</order>
		
		<finder name="COP_REPORT_NO" return-type="VRCOPReportRepository">
			<finder-column name="COPReportNo"/>
		</finder>

	</entity>
	
